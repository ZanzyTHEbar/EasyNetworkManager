{
    "name": "EasyNetworkManager",
    "keywords": "network, wifi, REST API, OTA, mDNS, MDNS, custom config, wifi manager, ESP32, ESP8266, frontend",
    "description": "An easy to use and extensible network manager for the ESP32 and ESP8266",
    "authors": [
        {
            "name": "ZanzyTHEbar",
            "url": "https://github.com/ZanzyTHEbar"
        }
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/ZanzyTHEbar/EasyNetworkManager.git"
    },
    "dependencies": [
        {
            "name": "ESPAsyncWebServer",
            "version": "https://github.com/me-no-dev/ESPAsyncWebServer.git",
            "platforms": [
                "espressif32"
            ]
        },
        {
            "name": "AsyncTCP",
            "version": "https://github.com/me-no-dev/AsyncTCP.git",
            "platforms": [
                "espressif32"
            ]
        },
        {
            "name": "ESPAsyncTCP",
            "version": "https://github.com/me-no-dev/ESPAsyncTCP.git",
            "platforms": [
                "espressif8266"
            ]
        },
        {
            "owner": "leftcoast",
            "name": "LC_baseTools",
            "version": "@^1.5"
        },
        {
            "name": "ArduinoJSON",
            "version": "https://github.com/bblanchon/ArduinoJson"
        }
    ],
    "includeDirs": [
        "src/api",
        "src/api/baseAPI",
        "src/api/utilities",
        "src/data",
        "src/data/config",
        "src/data/StateManager",
        "src/data/utilities",
        "src/mDNS",
        "src/ota",
        "src/wifihandler",
        "src/wifihandler/utilities"
    ],
    "headers": [
        "webserverHandler.hpp",
        "project_config.hpp",
        "StateManager.hpp",
        "enuminheritance.hpp",
        "makeunique.hpp",
        "Observer.hpp",
        "MDNSManager.hpp",
        "OTA.hpp",
        "wifiHandler.hpp"
    ],
    "version": "1.4.15",
    "frameworks": "arduino",
    "platforms": [
        "espressif32",
        "espressif8266"
    ]
}